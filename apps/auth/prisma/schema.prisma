generator client {
 provider = "prisma-client-js"
 output = "../node_modules/.prisma/client"
 binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model User {
    id Int @default(autoincrement()) @id
    email String
    password String
    roles String[]
}