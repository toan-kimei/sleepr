generator client {
 provider = "prisma-client-js"
 output = "../node_modules/.prisma/client"
 binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model Reservation {
    id Int @default(autoincrement()) @id
    timestamp DateTime
    startDate DateTime
    endDate DateTime
    userId Int
    invoiceId String
}